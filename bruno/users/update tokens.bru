meta {
  name: update tokens
  type: http
  seq: 3
}

post {
  url: {{url}}/api/auth/update-tokens
  body: none
  auth: none
}

script:pre-request {
  const cookie = bru.getVar("cookie");
  
  if(cookie) {
    req.setHeader("Cookie", cookie)
  }
}

script:post-response {
  const cookies = res.getHeader('set-cookie');
  
  if(cookies) {
    bru.setVar("cookie", cookies.join('; '));
  }
}
