package pages

import "github.com/DeepAung/deep-art/views/layouts"
import "github.com/DeepAung/deep-art/pkg/mytoken"
import "strconv"

templ Home(payload mytoken.Payload) {
	@layouts.Simple() {
		<h1>Home page</h1>
		<p>{ strconv.Itoa(payload.UserId) }</p>
		<p>{ payload.Username }</p>
		<p class="cursor-pointer" hx-post="/api/signout" hx-target-error="#error-text">Sign out</p>
		<p id="#error-text"></p>
	}
}
